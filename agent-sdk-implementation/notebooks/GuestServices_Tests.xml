<?xml version="1.0" ?>
<AiEvaluationDefinition xmlns="http://soap.sforce.com/2006/04/metadata">
  <fullName>GuestServices_Tests</fullName>
  <label>Guestservices Tests</label>
  <agentVersion>Resort Manager Agent</agentVersion>
  <testCases>
    <name>TestCase_1_ReservationLookup</name>
    <inputs>
      <utterance>Find my reservation for John Smith</utterance>
      <contextVariables>
        <name>CustomerType</name>
        <value>VIP</value>
      </contextVariables>
      <contextVariables>
        <name>Channel</name>
        <value>Phone</value>
      </contextVariables>
    </inputs>
    <expectations>
      <type>topicSequence</type>
      <expectedTopic>GuestServices</expectedTopic>
    </expectations>
    <expectations>
      <type>actionSequence</type>
      <expectedAction>search_reservations</expectedAction>
    </expectations>
  </testCases>
</AiEvaluationDefinition>
